<h1>Per#index</h1>


<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Admin status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <% if user != current_user %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.admin %></td>
        <td><%= link_to 'Show', user %></td>
        
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table>