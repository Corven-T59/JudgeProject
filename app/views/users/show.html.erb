
<div class="top-space"> </div>
<div class="row no-float">
  <div class="col-md-10 col-md-offset-1">
    <div class="panel">
      <div class="panel-heading" style="background-color: #1c3b6d;">
        <h3 class="panel-title white-text"><%= link_to 'Usuarios', users_path %> &raquo;  <%= @user.email %> </h3>
      </div>
      <div class="panel-body">

        <p>
          <strong>Administrador:</strong>
          <%= @user.admin %>
        </p>
      </div>
      <div class="panel-footer">
        <ul class="list-inline">
          <li><%= link_to 'Editar', edit_user_path(@user) %></li>
          <li><%= link_to 'Atras', users_path %></li>
        </ul>
      </div>
    </div>
  </div>
</div>
