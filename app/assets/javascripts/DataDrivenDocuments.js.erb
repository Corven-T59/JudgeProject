var stadistics = $("#user_answers_chart").data("stadistics");
if (stadistics) {
    var data = [
        {label: 'OK', val: stadistics.total_ok},
        {label: 'WA', val: stadistics.total_wa},
        {label: 'RE', val: stadistics.total_re},
        {label: 'CE', val: stadistics.total_ce},
        {label: 'TLE', val: stadistics.total_tle}
    ];

    //var data = [4, 8, 15, 16, 23, 42];

    var x = d3.scaleLinear()
        .domain([0, d3.max(data, function(d) { return d.val; })])
        .range([0, 420]);

    d3.select(".chart")
        .selectAll("div")
        .data(data, function(d) { return d.val; })
        .enter().append("div")
        .style("width", function(d) { return x(d.val) + "px"; })
        .text(function(d) { return d.label + ": "+d.val; });
}
